    <!--Reference_List: don’t worry about referencing the data raw files, etc, but do reference User Guides -->
    <Reference_List>
        <!-- Reference both types of mosaic for the associated metadata -->
        <Internal_Reference>
            <lid_reference>$MOSAIC_LID$</lid_reference>
            <reference_type>ancillary_to_data</reference_type>
            <comment>
            $MOSAIC_REFERENCE_COMMENT$
	        </comment>
        </Internal_Reference>
        <Internal_Reference>
            <lid_reference>$USERGUIDE_LID$</lid_reference>
            <reference_type>ancillary_to_document</reference_type>
            <comment>Detailed User's Guide</comment>
        </Internal_Reference>
        <Internal_Reference>
            <lid_reference>urn:nasa:pds:cassini_iss_saturn:document:iss-data-user-guide</lid_reference>
            <reference_type>ancillary_to_document</reference_type>
            <comment>The ISS Data User's Guide</comment> <!-- Needs DOI -->
        </Internal_Reference>
    </Reference_List>

    <File_Area_Ancillary>
        <!-- Reference the text file that lists all the source images that went into making mosaic -->
        <File>
            <!-- Source images, LIDs metadata -->
            <file_name>$IMAGE_TABLE_FILENAME$</file_name>
            <creation_date_time>$NOW$</creation_date_time>
            <md5_checksum>$IMAGE_TABLE_HASH$</md5_checksum>
        </File>
        <!-- Index for Source images (2 columns: Rob's index number for the image, LID) -->
        <!-- This file contains identifiers for the source images of this mosaic (to map longitudes back to source image) -->
        <Header>
            <!-- Optional, there are only two cols so not really needed-->
            <offset unit="byte">0</offset>
            <object_length unit="byte">26</object_length>
            <parsing_standard_id>UTF-8 Text</parsing_standard_id>
            <description>Source Image Index, LIDVID</description>
        </Header>
        <Table_Character>
            <offset unit="byte">23</offset> <!--This should match <Header>.<object_length> -->
            <records>$NUM_IMAGES$</records>
            <description>Source image indices and associated LIDVIDs</description>
            <record_delimiter>Line-Feed</record_delimiter>
            <Record_Character>
                <fields>2</fields>
                <groups>0</groups>
                <record_length unit="byte">66</record_length>
                <Field_Character>
                    <name>Source Image Index</name>
                    <field_number>1</field_number>
                    <field_location unit="byte">1</field_location>
                    <data_type>ASCII_NonNegative_Integer</data_type>
                    <field_length unit="byte">4</field_length>
                    <unit>none</unit> <!-- I think could simply delete this, if no units? -->
                    <description>The index number specific to this mosaic for the source image.
		              A source image can appear in multiple mosaics and therefore will be referenced by different index numbers.
	                </description>
                </Field_Character>
                <Field_Character>
                    <name>LIDVID</name>
                    <field_number>2</field_number>
                    <field_location unit="byte">10</field_location>
                    <data_type>ASCII_Real</data_type> <!-- XXX What should this be? String of some kind -->
                    <field_length unit="byte">59</field_length> <!-- MJTM: update -->
                    <unit>none</unit>  <!-- I think I could simply delete this, if no units? -->
                    <description>LIDVID for the source image.</description>
                </Field_Character>
            </Record_Character>
        </Table_Character>
    </File_Area_Ancillary>
    <!--Metadata, up to 18000 rows, seven columns:
	Co-rotating longitude, Source Image Index, Mid-time SPICE ET, Inertial longitude, Resolution, Phase angle, Emission angle -->
    <File_Area_Ancillary>
        <File>
            <!-- Metadata parameters per longitude value -->
            <file_name>$METADATA_PARAMS_TABLE_FILENAME$</file_name>
            <creation_date_time>$NOW$</creation_date_time>
            <md5_checksum>$METADATA_PARAMS_TABLE_HASH$</md5_checksum>
        </File>
        <Header>
            <!-- Optional to have column names -->
            <offset unit="byte">0</offset>
            <object_length unit="byte">113</object_length>
            <parsing_standard_id>UTF-8 Text</parsing_standard_id>
            <description>Corotating Longitude, Image Index, Mid-time SPICE ET, Inertial Longitude, Resolution, Phase Angle, Emission Angle</description>
        </Header>
        <!-- Metadata table. Fixed width, with <comma><whitespace> field separators -->
        <Table_Character>
            <offset unit="byte">121</offset> <!-- MJTM update: This should match <Header>.<object_length> -->
            <records>$NUM_VALID_LONGITUDES$</records>
            <description>Metadata for each co-rotating longtudinal value... etc ... </description>  <!-- MJTM update -->
            <record_delimiter>Line-Feed</record_delimiter>
            <Record_Character>
                <fields>7</fields>
                <groups>0</groups>
                <record_length unit="byte">72</record_length> <!-- Including <comma><whitespace>s -->
                <Field_Character>
                    <name>Corotating Longitude</name> <!-- Incrementing by 0.02 deg as read column top to bottom -->
                    <field_number>1</field_number>
                    <field_location unit="byte">1</field_location>
                    <data_type>ASCII_Real</data_type>
                    <field_length unit="byte">6</field_length>
                    <unit>deg</unit>
                    <description>Description of coord sys and corotating longitude</description>
                </Field_Character>
                <Field_Character>
                    <name>Image Index</name>
                    <field_number>2</field_number>
                    <field_location unit="byte">9</field_location> <!-- added 2 bytes for <comma><whitespace> -->
                    <data_type>ASCII_NonNegative_Integer</data_type>
                    <field_length unit="byte">4</field_length>
                    <unit>none</unit>
                    <description>Source image index</description>
                </Field_Character>
                <Field_Character>
                    <name>Mid-time SPICE ET</name>
                    <field_number>3</field_number>
                    <field_location unit="byte">15</field_location> <!-- added 2 bytes for <comma><whitespace> -->
                    <data_type>ASCII_Real</data_type>
                    <field_length unit="byte">13</field_length>
                    <unit>s</unit>
                    <description>Double precision number of TDB seconds past the J2000 epoch</description>
                </Field_Character>
                <Field_Character>
                    <name>Inertial Longitude</name>
                    <field_number>4</field_number>
                    <field_location unit="byte">30</field_location> <!-- added 2 bytes for <comma><whitespace> -->
                    <data_type>ASCII_Real</data_type>
                    <field_length unit="byte">7</field_length>
                    <unit>deg</unit>
                    <description>Description of Longitude as measured in inertial frame</description>
                </Field_Character>
                <Field_Character>
                    <name>Resolution</name>
                    <field_number>5</field_number>
                    <field_location unit="byte">39</field_location> <!-- added 2 bytes for <comma><whitespace> -->
                    <data_type>ASCII_Real</data_type>
                    <field_length unit="byte">10</field_length>
                    <unit>km/pixel</unit>
                    <description>Radial resolution?</description>
                </Field_Character>
                <Field_Character>
                    <name>Phase Angle</name>
                    <field_number>6</field_number>
                    <field_location unit="byte">51</field_location> <!-- added 2 bytes for <comma><whitespace> -->
                    <data_type>ASCII_Real</data_type>
                    <field_length unit="byte">10</field_length>
                    <unit>deg</unit>
                    <description>Description of phase angle</description>
                </Field_Character>
                <Field_Character>
                    <name>Emission Angle</name>
                    <field_number>7</field_number>
                    <field_location unit="byte">63</field_location> <!-- added 2 bytes for <comma><whitespace> -->
                    <data_type>ASCII_Real</data_type>
                    <field_length unit="byte">10</field_length>
                    <unit>deg</unit>
                    <description>Description of emission angle</description>
                </Field_Character>
            </Record_Character>
        </Table_Character>
    </File_Area_Ancillary>
</Product_Ancillary>