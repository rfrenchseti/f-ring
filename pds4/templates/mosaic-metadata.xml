                        <local_identifier>reproj_geom</local_identifier>
                        <rings:description>
$MOSAIC_METADATA_RINGS:DESCRIPTION$
			            </rings:description>
                        <rings:epoch_reprojection_basis_utc unit="s">2007-01-01T00:00:00.000Z</rings:epoch_reprojection_basis_utc>
